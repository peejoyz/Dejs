
<% include ../partials/admin_header.ejs %>

 <h3 class="page-title">Add a category</h3>
 <a href="/admin/categories" class="btn btn-md btn-primary">Back to all categories</a>
<br><br>

<form method="post" action="/admin/categories/add-category">

    <div class="form-group">
        <label for="">Title</label>
        <input type="text" class="form-control" name="title" id="title" value="<%= title %>" placeholder="Title">
    </div>

    <button class="btn btn-default">Submit</button>
</form>

